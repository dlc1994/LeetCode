	//题目描述
	//从上往下打印出二叉树的每个节点，同层节点从左至右打印。
	vector<int> PrintFromTopToBottom(TreeNode* root) {
		vector<int> res;
		if(root==NULL) return NULL;
		deque<TreeNode*> d;
		d.push_back(root);
		while(!d.empty()){
			root=d.front();
			if(root!=NULL){
				res.push_back(root->val);
				d.push_back(root->left);
				d.push_back(root->right);
			}
			d.pop_front();
		}
		return res;
    }
